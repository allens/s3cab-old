{
  "name": "s3cab",
  "description": "Content addressable backups to S3 with open and portable data format",
  "version": "0.0.0",
  "author": "Allen Shiels @allens",
  "bin": {
    "s3cab": "./bin/run"
  },
  "bugs": "https://github.com/allens/s3cab/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "globby": "^11.0.4",
    "jest": "^27.3.1",
    "prettier": "2.5.0",
    "rimraf": "^3.0.2",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": "^14.18.1"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/allens/s3cab",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "s3cab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "allens/s3cab",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.42.0",
    "@aws-sdk/credential-provider-ini": "^3.41.0",
    "@oclif/command": "^1.8.4",
    "@oclif/config": "^1.17.1",
    "@oclif/plugin-help": "^3.2.7",
    "cli-ux": "^6.0.6",
    "tslib": "^2.3.1"
  }
}
