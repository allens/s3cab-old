{
  "name": "s3cab",
  "description": "Content addressable backups to S3 with open and portable data format",
  "version": "0.0.0",
  "author": "Allen Shiels @allens",
  "bin": {
    "s3cab": "./bin/run"
  },
  "bugs": "https://github.com/allens/s3cab/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "eslint": "^8.2.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11.0.4",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/allens/s3cab",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "s3cab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "allens/s3cab",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.3",
    "@oclif/config": "^1.17.1",
    "@oclif/plugin-help": "^3.2.5",
    "tslib": "^2.3.1"
  }
}